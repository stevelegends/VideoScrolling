{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "android:build:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "prepare": "husky",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier . --write"
  },
  "lint-staged": {
    "*.js": "eslint --fix ",
    "*.{js,css,md,html,json}": "prettier --cache --write"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.8.1",
    "@react-navigation/bottom-tabs": "^7.0.0-rc.32",
    "@react-navigation/elements": "^2.0.0-rc.24",
    "@react-navigation/native": "^7.0.0-rc.19",
    "@react-navigation/native-stack": "^7.0.0-rc.27",
    "@shopify/flash-list": "^1.7.1",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
